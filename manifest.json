{
	"manifest_version": 2,
	"name": "Dereferer Button",
	"description": "Adds a button to switch whether HTTP referers should be sent or not.",
	"version": "0.1.0",
	"background": {
		"scripts": [
			"background.js"
		]
	},
	"browser_action": {
		"default_icon": "icons/deref-working-64.png",
		"default_title": "Dereferer"
	},
	"permissions": [
		"webRequest",
		"webRequestBlocking"
	]
}